<h1>Product List</h1>
<table border="1">
   <tr>
      <th>Product name</th>
      <th>Product price</th>
      <th>Product image</th>
      <th>Product category</th>
      <th>Menu</th>
   </tr>
   {{#each productList }}
   <tr>
      <td>{{ name }}</td>
      <td>${{ price }}</td>
      <td>
         <img src="{{ image }}" width="100" height="100"></td>
      <td>{{ category.name }}</td>
      <td>
         <a href="http://">Edit</a>
         <a href="http://">Delete</a>
      </td>
   </tr>
   {{/each}}
</table>